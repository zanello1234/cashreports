<odoo>
    <record id="action_cashbox_session_report" model="ir.actions.report">
        <field name="name">Reporte de Sesiones de Caja</field>
        <field name="model">account.cashbox.session</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">account_cashbox_reports.cashbox_session_report_template</field>
        <field name="report_file">account_cashbox_reports.cashbox_session_report_template</field>
        <field name="binding_model_id" ref="account_cashbox.model_account_cashbox_session"/>
    </record>
    
    <record id="action_report_cashbox_session_summary" model="ir.actions.report">
        <field name="name">Resumen de caja</field>
        <field name="model">account.cashbox.session</field>
        <field name="binding_model_id" ref="account_cashbox.model_account_cashbox_session"/>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">account_cashbox_reports.report_session_summary</field>
        <field name="report_file">account_cashbox_reports.report_session_summary</field>
        <field name="binding_type">report</field>
    </record>
</odoo>
